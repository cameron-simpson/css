#!/bin/sh
which python3.3 >/dev/null 2>&1 && exec python3.3 ${1+"$@"}
which python3.2 >/dev/null 2>&1 && exec python3.2 ${1+"$@"}
which python3.1 >/dev/null 2>&1 && exec python3.1 ${1+"$@"}
which python3.0 >/dev/null 2>&1 && exec python3.0 ${1+"$@"}
echo "$0: no python 3.3, 3.2, 3.1 or 3.0, aborting: $0 $*" >&2
exit 1
