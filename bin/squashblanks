from __future__ import print_function
import sys

had_blank = False
for line in sys.stdin:
  oline = line
  line = line.rstrip()
  line = line.expandtabs()
  if len(line) == 0:
    if had_blank:
      continue
    had_blank = True
  else:
    had_blank = False
  print(line)
