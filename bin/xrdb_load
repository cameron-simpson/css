#!/bin/sh

. $HOME/rc/x11/xenv.sh

case "$X11_DEPTH,$X11_CLASS" in
    ,)				type=Colour ;;	# guess, not recurse
    1,*)			type=Mono ;;
    *,StaticGray|*,GrayScale)	type=Grey ;;
    *)				type=Colour ;;
esac

cd $HOME/rc/x11 || exit $?

uncurly `ls -d dflt/* | grep -v '^dflt/CVS$'` \
| xrdb -nocpp -load -

##dpp ${1+"$@"} $X11/xinfo.dpp $X11/defaults | xrdb -load -nocpp
