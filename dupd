#!/bin/sh
#
# Notice changes in disc usage.
#	- Cameron Simpson <cs@zip.com.au>
#
# Timestamping.	- cameron, 21mar1999
#

now=`datecode`
dusum "$now"

>>.du-a
>>.du-s
dudiff .du-a ".du-a.$now" > .du-a.diff
dudiff .du-s ".du-s.$now" > .du-s.diff

rm .du-a;ln -s ".du-a.$now" .du-a
rm .du-s;ln -s ".du-s.$now" .du-s
