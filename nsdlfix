#!/bin/sh
#
# Netscape has an unfortunate tendency to uncompress downloaded
# files and, better still, doesn't strip off the .Z or .gz extension.
# Thus this hack to clean up.
#	- Cameron Simpson <cs@zip.com.au> 04dec98
#

[ $# = 0 ] && { set x `ls -d *.gz *.Z 2>/dev/null`; shift; }

xit=0
for file
do  if [ ! -r "$file" ]
    then  echo "$0: not a readable file" >&2; xit=1; continue
    fi

    type=`file "$file"` || { xit=1; continue; }

    case $type in
	*USTAR*)	
    esac
