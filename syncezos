#!/bin/sh
#
# Update my ezoshosting web site.
#	- Cameron Simpson <cs@zip.com.au> 11jan2003
set -x

syncezos-rc
syncezos-cgi

rsync -avHPW --delete --exclude=/kath $HOME/there/cskk.ezoshosting.com/html/. ezos:public_html/.
rsync -avHPW --delete $HOME/there/cskk.ezoshosting.com/cgi-bin/. ezos:cgi-bin/.

syncsmall @ezos
